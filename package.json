{
  "name": "astropsick",
  "type": "module",
  "main": "index.ts",
  "repository": "https://github.com/Frikadellios/Astropsick",
  "author": "Hrihorii Ilin <devopsick@pm.me>",
  "license": "MIT",
  "version": "1.0.1",
  "scripts": {
    "preinst": "npm install -g bun && yarn && bun install --yarn",
    "postbuild": "pagefind --site dist",
    "format:check": "prettier --plugin-search-dir=. --check .",
    "format": "prettier --plugin-search-dir=. --write .",
    "lint": "eslint .",
    "syncyarn": "bun bun.lockb",
    "dev": "bunx --bun astro dev --open",
    "start": "astro check && astro dev --verbose",
    "build": "astro build && pagefind --site dist",
    "preview": "astro preview",
    "astro": "astro",
    "test": "bun test --watch",
    "new-post": "node scripts/new-post.js",
    "biome": "bun run biomelint && bun run biomeformat",
    "biomeformat": "biome format --write ./src",
    "biomelint": "biome check --apply ./src"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.4",
    "@astrojs/mdx": "2.1.1",
    "@astrojs/node": "8.2.0",
    "@astrojs/react": "^3.0.10",
    "@astrojs/rss": "4.0.5",
    "@astrojs/sitemap": "3.0.5",
    "@astrojs/tailwind": "5.1.0",
    "@astrojs/ts-plugin": "1.4.0",
    "@astrojs/vercel": "^6.1.0",
    "@fontsource/inter": "^4.5.15",
    "@fontsource/montserrat": "^4.5.14",
    "@fontsource/titan-one": "^4.5.9",
    "@forastro/utilities": "^2.2.4",
    "@happy-dom/global-registrator": "^12.10.3",
    "@tailwindcss/typography": "^0.5.10",
    "@types/canvas-confetti": "^1.6.4",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "animate.css": "^4.1.1",
    "astro": "4.3.5",
    "astro-relative-links": "^0.3.3",
    "canvas-confetti": "^1.9.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "crypto": "^1.0.1",
    "jest": "^29.7.0",
    "jose": "^5.2.0",
    "lint-staged": "15.2.2",
    "lottie-react": "^2.4.0",
    "lottie-web": "^5.11.0",
    "lucide-react": "^0.323.0",
    "mdast-util-to-string": "^4.0.0",
    "million": "^3.0.2",
    "motion": "^10.16.4",
    "nanoid": "^5.0.4",
    "overlayscrollbars": "^2.4.6",
    "pagefind": "^1.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "sharp": "^0.31.3",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-animated": "^1.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "peerDependencies": {
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@rollup/plugin-yaml": "^4.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/bun": "^1.0.5",
    "@typescript-eslint/parser": "6.21.0",
    "bun-types": "latest",
    "eslint": "^8.53.0",
    "eslint-plugin-astro": "0.31.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "msw": "^2.0.11",
    "prettier": "^3.2.5",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.11"
  },
  "lint-staged": {
    "*.{astro,js,jsx,ts,tsx,md,mdx,json}": [
      "prettier --write --plugin-search-dir=."
    ]
  }
}
